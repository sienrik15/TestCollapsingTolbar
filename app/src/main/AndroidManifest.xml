<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.info.testcollapseactionbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.info.testcollapseactionbar.activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.info.testcollapseactionbar.activities.ExpandableToolBarActivity"
            android:label="@string/expand" />
        <activity
            android:name="com.info.testcollapseactionbar.activities.HiddenToolbarActivity"
            android:label="@string/hide" />
        <activity
            android:name="com.info.testcollapseactionbar.activities.ParalaxToobarActivity"
            android:label="@string/paralax" />
    </application>

</manifest>
